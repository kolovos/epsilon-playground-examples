<?nsuri stm?>
<!--
A state machine with three states A, B and C
and transitions among them
-->
<machine name="ABC">
    <state name="A"/>
    <state name="B"/>
    <state name="C"/>
    <transition from="A" to="B"/>
    <transition from="B" to="C"/>
    <transition from="A" to="A"/>
    <transition from="C" to="A"/>
</machine>